{
  "extends": "@polkadot/dev/config/tsconfig.json",
  "compilerOptions": {
    "composite": true,
    "paths": {
      "@subwallet/sub-connect": ["sub-connect/src"],
      "@subwallet/sub-connect/*": ["sub-connect/src/*"],
      "@subwallet/wallet-connect": ["wallet-connect/src"],
      "@subwallet/wallet-connect/*": ["wallet-connect/src/*"],
      "@subwallet/web3-react-subwallet-connector-v6": ["web3-react-subwallet-connector-v6/src"],
      "@subwallet/web3-react-subwallet-connector-v6/*": ["web3-react-subwallet-connector-v6/src/*"],
      "@subwallet/wagmi-connector": ["wagmi-connector/src"],
      "@subwallet/wagmi-connector/*": ["wagmi-connector/src/*"]
    },
    "skipLibCheck": true
  }
 }
